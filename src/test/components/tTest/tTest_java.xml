      <DEFAULT>false</DEFAULT> 
    </PARAMETER>  
    <PARAMETER FIELD="FILE" NAME="FILENAME" NUM_ROW="20" REQUIRED="true" REPOSITORY_VALUE="FILE_PATH" SHOW_IF="CREATE_EMPTY_WORKBOOK == 'false'" GROUP="FILE_GROUP"> 
      <DEFAULT>"/path/to/excel_input_file"</DEFAULT> 
    </PARAMETER>  
    <PARAMETER NAME="SPREADSHEET_TYPE" FIELD="CLOSED_LIST" NUM_ROW="20" SHOW_IF="CREATE_EMPTY_WORKBOOK == 'true'" GROUP="FILE_GROUP"> 
      <ITEMS DEFAULT="XLS"> 
        <ITEM NAME="XLS" VALUE="XLS"/>  
